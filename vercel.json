{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://chat-messenger-client.vercel.app",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, x-refresh-token, x-csrf-token",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
