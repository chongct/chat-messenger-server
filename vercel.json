{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "headers": {
    "source": "/(.*)",
    "headers": [
      {
        "key": "Access-Control-Allow-Origin",
        "value": "https://chat-messenger-client.vercel.app"
      }
    ]
  }
}
